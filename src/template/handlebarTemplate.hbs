 <div class="photo-card">
     {{#each this}}
  <img src="{{webformatURL}}" alt="" loading="lazy" />
  <div class="info">
    <p class="info-item">
      <b>{{likes}}</b>
    </p>
    <p class="info-item">
      <b>{{views}}</b>
    </p>
    <p class="info-item">
      <b>{{comments}}</b>
    </p>
    <p class="info-item">
      <b>{{downloads}}</b>
    </p>
  </div>
       {{/each}}
</div>